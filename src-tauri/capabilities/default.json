  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:allow-start-dragging",
    "opener:default",
    "opener:allow-open-url",
    "window-state:default",
    "updater:default",
    "updater:allow-check",
    "dialog:default"
  ]
}
